---
permalink: ./pages.json
permalinkBypassOutputDir: true
eleventyExcludeFromCollections: true
---
[
{%- for pages in collections.all %}
	{
      "selectors": [
            {
                  "selector": "#j-title",
                  "content":"{% if pages.data.title %}{{ pages.data.title | addNbsp | safe }}{% else %}11ty Rocks{% endif %}"
            },
            {
                  "selector": "#j-subtitle",
                  "content":"{% if pages.data.subtitle %}{{ pages.data.subtitle | addNbsp | safe }}{% else %}Insert here your subtitle{% endif %}"
            }
      ],
      "bodyClass": "t-palette--4",
      "imgName":"{% if pages.data.title %}{{ pages.data.title | slug }}{% else %}home{% endif %}"
	}{% if loop.last == false %},{% endif -%}
{% endfor %}
]
